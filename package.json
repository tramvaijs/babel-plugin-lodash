{
  "name": "@tinkoff/babel-plugin-lodash",
  "version": "0.1.0",
  "description": "Transform lodash imports to include into bundle only imported functions",
  "keywords": [
    "babel-plugin",
    "lodash"
  ],
  "private": true,
  "author": "Ivan Saranchonkau <i.saranchenkov@tbank.ru> (https://github.com/saranchonkau)",
  "repository": "https://github.com/tramvaijs/babel-plugin-lodash",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.18.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/helper-plugin-test-runner": "^7.24.7",
    "@babel/plugin-proposal-function-bind": "^7.24.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/plugin-syntax-typescript": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/types": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.50",
    "babel-plugin-lodash": "^3.3.4",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^3.3.3",
    "rollup": "^4.22.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  }
}
